{
  "name": "fibonacci-project-dashboard",
  "version": "1.0.0",
  "description": "Scalable cloud-native dashboard for project communication tracking and analysis",
  "main": "src/dashboard/index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src --ext .js,.ts,.vue",
    "lint:fix": "eslint src --ext .js,.ts,.vue --fix",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && vercel --prod",
    "deploy:preview": "npm run build && vercel",
    "analyze": "npm run build -- --analyze",
    "clean": "rm -rf dist node_modules/.vite",
    "start": "npm run dev"
  },
  "dependencies": {
    "@vercel/kv": "^1.0.1",
    "date-fns": "^2.30.0",
    "fuse.js": "^7.0.0",
    "chart.js": "^4.4.0",
    "marked": "^9.1.6"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6"
  },
  "keywords": [
    "dashboard",
    "project-management",
    "communication-tracking",
    "knowledge-base",
    "google-apps-script",
    "vercel",
    "scalable"
  ],
  "author": "Fibonacci Project Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/fibonacci-project-dashboard.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
